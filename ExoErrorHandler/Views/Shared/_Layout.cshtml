<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/exolutus/css/master.min.css" rel="stylesheet" />
    <link href="~/Content/themify/themify.css" rel="stylesheet">
    <link href="~/Content/bootstrap/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/kendo/kendo.common-bootstrap.core.min.css" rel="stylesheet" />
    <link href="~/Content/kendo/kendo.common-material.min.css" rel="stylesheet">
    <link href="~/Content/kendo/kendo.material.min.css" rel="stylesheet">
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet">
    <link href="~/Content/scrollable/asScrollable.min.css" rel="stylesheet">
    <link href="~/Content/exolutus/css/master.min.css" rel="stylesheet">



    @*<link href="~/Content/kendo/select2.css" rel="stylesheet" />*@
    @if (IsSectionDefined("StyleSheet"))
    {
        @RenderSection("StyleSheet", required: false)}
    <script src="~/Content/jquery/jquery.min.js"></script>
    <script src="~/Content/jquery/jquery.form.js"></script>
    <script src="~/Content/jquery/match-height.min.js"></script>
    <script src="~/Content/bootstrap/bootstrap.min.js"></script>
    <script src="~/Content/jquery/jquery.validate.min.js"></script>
    <script src="~/Content/jquery/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Content/jquery/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Content/bootstrap/bootstrap.min.js"></script>




    <script src="~/Content/kendo/kendo.all.min.js"></script>
    <script src="~/Content/kendo/kendo.aspnetmvc.min.js"></script>





    <script src="~/Content/bootbox/bootbox.min.js"></script>
    <script src="~/Content/toastr/toastr.min.js"></script>
    <script src="~/Content/toastr/custom.js"></script>
    <script src="~/Content/scrollable/asScrollable.min.js"></script>
    <script src="~/Content/exolutus/js/scripts.js"></script>
    <script src="~/Content/exolutus/js/Main.js"></script>



    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.5/dist/jquery.validate.js"></script>
</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-dark">
        <div class="container">
            @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            <button type="button" class="navbar-toggler" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" title="Toggle navigation" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("User", "Index", "User", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("Organization", "Index", "Organization", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("Project", "Index", "Project", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("Module", "Index", "Module", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("IssueError", "Index", "IssueError", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("About", "About", "Home", new { area = "" }, new { @class = "nav-link" })</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home", new { area = "" }, new { @class = "nav-link" })</li>

                </ul>
            </div>
        </div>
    </nav>

    @RenderBody()
    


    @RenderSection("scripts", required: false)
</body>
</html>
